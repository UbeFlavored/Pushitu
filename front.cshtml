@{
    Validation.RequireField("fname", "First name is required.");
    Validation.RequireField("lname", "Last name is required.");
    Validation.RequireField("emailad", "Email is required.");
    Validation.RequireField("pass1", "Password is required.");    
    Validation.RequireField("pass2", "Please confirm password");
  


    var db = Database.Open("SchedersDB");
    var fname = "";
    var lname = "";
    var emailad = "";
    var pass1 = "";
    var pass2 = "";
        
        fname = Request["fname"];
        lname = Request["lname"];
        emailad = Request["emailad"];    
        pass1 = Request["pass1"];        
        pass2 = Request["pass2"];

    

    if (IsPost) {

        // Define the insert query. The values to assign to the
        // columns in the Product table are defined as parameters
        // with the VALUES keyword.
        if(Validation.IsValid()) {
            var insertQuery = "INSERT INTO Users (FirstName, LastName, Email, Pass) " +
                "VALUES (@0, @1, @2, @3)";
            db.Execute(insertQuery, fname, lname, emailad, pass1);
            // Display the page that lists products.
            //new JavascriptResult { Script = "alert('Successfully registered');" };

            var id = db.GetLastInsertId();

            Response.Redirect("~/welcome/1?id=" + id);
        }
    }
}

