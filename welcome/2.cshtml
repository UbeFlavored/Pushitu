@{
    var db = Database.Open("SchedersDB");
    var id = Request.Params["id"];
    var i = 0;
    var getQuery = "SELECT tableID FROM ForeignKeys WHERE userID = " + id + " AND type = 'college'";
    var data = db.Query(getQuery);
    List<int> list = new List<int>();
    List<string> courses = new List<string>();
    
    foreach (var row in data)
    {
        list.Add(row.tableID);
        getQuery = "SELECT Name FROM Courses WHERE collegeID = " + row.tableID;
        var data2 = db.Query(getQuery);
        foreach (var row2 in data2)
        {   
            courses.Add(row2.Name);
        }
    }

    //data = db.Query(getQuery);



    if (IsPost)
    {
        var j = 0;
        foreach (var row in courses)
        {
            if (Request.Form[row] == "on")
            {
                var query = "SELECT ID FROM Courses WHERE Name = '" + row + "'";
                var data2 = db.Query(query);
                query = "INSERT INTO ForeignKeys (userID, type, tableID) " +
     "VALUES (" + id + ", 'courses', " + data2.First().ID + ")";
                db.Execute(query);
            }
            i++;
        }
        Response.Redirect("~/welcome/3?id=" + id);
    }
}

